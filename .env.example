# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# External APIs
# 국세청 API - 공공데이터포털에서 발급받은 API 키 (Decoding 버전 사용)
NTS_API_KEY=your_nts_api_key_from_data_go_kr
# 비즈노 API - 회사명 조회 (일일 200건 무료)
BIZNO_API_KEY=your_bizno_api_key
BUSINESS_VERIFICATION_TEST_MODE=false

# Payment (Stripe)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Stripe Price IDs (달러 결제)
NEXT_PUBLIC_STRIPE_PRICE_PROSPECTIVE_FOUNDER=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_FOUNDER=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_VC_GENERAL=price_xxx
NEXT_PUBLIC_STRIPE_PRICE_VC_ANONYMOUS=price_xxx

# App URL
NEXT_PUBLIC_APP_URL=https://your-app.netlify.app

# Email
EMAIL_FROM=noreply@sogon.vc